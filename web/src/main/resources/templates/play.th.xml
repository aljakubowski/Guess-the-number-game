<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>

    <attr sel="#main-message"
          th:text="${__${T(alja.programming.util.AttributeNames).MAIN_MESSAGE}__}"/>

    <attr sel="#result-message"
          th:text="${__${T(alja.programming.util.AttributeNames).RESULT_MESSAGE}__}"/>

    <attr sel="#game-form"
          th:action="@{__${T(alja.programming.util.GameMappings).PLAY}__}"/>

    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footerFragment}"/>

</thlogic>

